<h1>All Venues</h1>
<form action="/insert_venue_record" method="post">
  <%= csrf_meta_tags %>
  <label for="name">Name</label>
  <input type="text" id="name" name="venue[name]">
  
  <label for="address">Address</label>
  <input type="text" id="address" name="venue[address]">
  
  <label for="neighborhood">Neighborhood</label>
  <input type="text" id="neighborhood" name="venue[neighborhood]">
  
  <button type="submit">Add venue</button>
</form>
<ul>
  <% @venues.each do |venue| %>
    <li>
      <%= link_to venue.name, venue_path(venue.id) %>
    </li>
  <% end %>
</ul>
<p><%= link_to 'View all users', users_path %></p>
